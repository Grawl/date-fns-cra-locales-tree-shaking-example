<!-- Manifest -->
<% if (isProduction) { %>
    <link rel="apple-touch-icon" sizes="180x180" href="<%= PUBLIC_URL %>/manifest/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="<%= PUBLIC_URL %>/manifest/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="<%= PUBLIC_URL %>/manifest/favicon-16x16.png">
    <link rel="manifest" href="<%= PUBLIC_URL %>/manifest/site.webmanifest">
    <link rel="shortcut icon" href="<%= PUBLIC_URL %>/favicon.ico">
    <meta name="apple-mobile-web-app-title" content="<%= TITLE %>">
    <meta name="application-name" content="<%= TITLE %>">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-config" content="<%= PUBLIC_URL %>/manifest/browserconfig.xml">
    <meta name="theme-color" content="#ffffff">
    <link rel="shortcut icon" href="<%= PUBLIC_URL %>/manifest/favicon.svg" />
<% } else { %>
    <link rel="apple-touch-icon" sizes="180x180" href="<%= PUBLIC_URL %>/manifest/dev/apple-touch-icon.png">
    <link rel="icon" type="image/png" sizes="32x32" href="<%= PUBLIC_URL %>/manifest/dev/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="16x16" href="<%= PUBLIC_URL %>/manifest/dev/favicon-16x16.png">
    <link rel="manifest" href="<%= PUBLIC_URL %>/manifest/dev/site.webmanifest">
    <link rel="shortcut icon" href="<%= PUBLIC_URL %>/manifest/dev/favicon.ico">
    <meta name="apple-mobile-web-app-title" content="<%= TITLE %>">
    <meta name="application-name" content="<%= TITLE %>">
    <meta name="msapplication-TileColor" content="#ca6363">
    <meta name="msapplication-config" content="<%= PUBLIC_URL %>/manifest/dev/browserconfig.xml">
    <meta name="theme-color" content="#ca6363">
    <link rel="shortcut icon" href="<%= PUBLIC_URL %>/manifest/dev/favicon.svg" />
<% } %>
<link rel="mask-icon" href="<%= PUBLIC_URL %>/manifest/safari-pinned-tab.svg" color="#ca6363">
<!-- / Manifest -->
